{
    "extends": ["airbnb"],
    "plugins": ["react", "jsx-a11y", "import"],
    "env": {
      "browser": true,
      "es2021": true,
      "jest": true
    },
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "react/react-in-jsx-scope": "off",
      "import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
    }
  }
  